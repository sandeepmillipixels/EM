<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/onboarding_background"
    android:layout_height="match_parent">



    <RelativeLayout
        android:layout_below="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_120_dp"
        android:layout_height="wrap_content">


        <com.application.millipixels.expense_rocket.typeface.FontsClass
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/textView2"
            android:textColor="@color/white"
            android:gravity="center|left"
            android:layout_marginLeft="@dimen/margin_30_dp"
            android:text="@string/did_i_mail"
            android:textSize="@dimen/size_30dp" />


        <com.application.millipixels.expense_rocket.typeface.FontsClassLight
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_centerHorizontal="true"
            android:textSize="@dimen/size_14dp"
            android:layout_marginLeft="@dimen/margin_60_dp"
            android:layout_marginTop="@dimen/margin_8_dp"
            android:textColor="@color/white"
            android:gravity="center|left"
            android:text="@string/oops_moment" />






    </RelativeLayout>





</RelativeLayout>